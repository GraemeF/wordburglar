{
  "name": "wordburglar",
  "version": "0.0.0-21",
  "description": "A game of many words.",
  "main": "index.js",
  "scripts": {
    "start": "node ./wordburglar.js",
    "test": "node ./node_modules/mocha/bin/mocha",
    "lint": "node ./node_modules/jshint/bin/hint ./lib ./test/unit ./test/endtoend ./test/endtoend/helpers --show-non-errors",
    "cover": "node ./node_modules/cover/bin/cover run node_modules/mocha/bin/_mocha && node ./node_modules/cover/bin/cover report html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GraemeF/wordburglar.git"
  },
  "keywords": [
    "game"
  ],
  "author": "Graeme Foster <graemef@gmail.com>",
  "private": true,
  "devDependencies": {
    "mocha": "~1.5.0",
    "sinon": "~1.4.2",
    "chai": "~1.2.0",
    "patience": "~0.2.0",
    "sinon-chai": "~2.1.2",
    "zombie": "~2.0.0-alpha1",
    "jshint": "~0.9.0",
    "cover": "~0.2.8"
  },
  "dependencies": {
    "nconf": "~0.6.4",
    "express": "~3.0.0rc4",
    "jamjs": "~0.2.5",
    "jade": "~0.27.2",
    "socket.io": "~0.9.10",
    "underscore": "~1.3.3"
  },
  "bundleDependencies": [
    "socket.io"
  ],
  "jam": {
    "baseUrl": "public",
    "packageDir": "public/assets",
    "dependencies": {
      "bootstrap": "2.0.x",
      "underscore": "1.3.x",
      "socket.io": "0.9.x",
      "backbone": "0.9.x",
      "jquery": "1.8.x"
    }
  },
  "subdomain": "wordburglar",
  "engines": {
    "node": "0.8.x"
  }
}
